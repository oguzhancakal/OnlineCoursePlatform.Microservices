name: dockercompose4504562990327466981
services:
  mongo.db.catalog:
    container_name: mongo.db.catalog.container
    environment:
      MONGO_INITDB_ROOT_PASSWORD: Password12
      MONGO_INITDB_ROOT_USERNAME: myuser
    image: mongo:8.0-rc
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27030"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: mongo.db.catalog.volume
        target: /data/db
        volume: {}
  mongo.db.catalog.ui:
    container_name: mongo.db.catalog.ui.container
    environment:
      ME_CONFIG_BASICAUTH: "false"
      ME_CONFIG_MONGODB_URL: mongodb://myuser:Password12@mongo.db.catalog:27017/
    image: mongo-express:1.0.2-20-alpine3.19
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8081
        published: "27032"
        protocol: tcp
    restart: always
networks:
  default:
    name: dockercompose4504562990327466981_default
volumes:
  mongo.db.catalog.volume:
    name: dockercompose4504562990327466981_mongo.db.catalog.volume